<UserControl x:Class="BinaryTreeVisualizer.TreeNode"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BinaryTreeVisualizer"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="50">
    <Grid>
            <Canvas>
            <Grid Height="50" Width="50">
                <Ellipse Name="Ell" Width="50" Height="50" Stroke="Black" StrokeThickness="1"></Ellipse>
                <TextBlock Name="TxtBox" HorizontalAlignment="Center" VerticalAlignment="Center" TextWrapping="Wrap" TextAlignment="Center" ></TextBlock>

            </Grid>
        </Canvas>
    </Grid>
</UserControl>
